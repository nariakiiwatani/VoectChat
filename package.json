{
  "name": "default",
  "version": "0.1.0",
  "private": true,
  "main": "dist/functions/index.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev:nextjs": "next src/nextjs",
    "dev:functions": "babel src/functions --out-dir dist/functions --source-maps --watch",
    "dev": "npm run dev:nextjs & npm run dev:functions",
    "build:nextjs": "next build src/nextjs",
    "build:functions": "babel src/functions --out-dir dist/functions --source-maps",
    "build": "npm run build:nextjs && npm run build:functions",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=production firebase serve --only functions,hosting",
    "predeploy": "rimraf dist/ && npm run build",
    "deploy": "cross-env NODE_ENV=production firebase deploy --only functions,hosting",
    "build:now": "rimraf .next/ && cross-env DEPLOYMENT_TARGET=now npm run build:nextjs && cp -r dist/nextjs/ .next/"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@nandorojo/swr-firestore": "^0.7.8",
    "color": "^3.1.2",
    "color-convert": "^1.9.1",
    "color-string": "^1.5.2",
    "firebase": "^7.14.6",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.6.2",
    "next": "^9.4.4",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-jsx": "^3.3.0",
    "styled-jsx-plugin-sass": "^1.0.0",
    "ts-node": "^8.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@types/color": "^3.0.1",
    "@types/color-convert": "^1.9.0",
    "@types/color-string": "^1.5.0",
    "@types/next": "^9.0.0",
    "@types/node": "^14.0.9",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "@types/styled-jsx": "^2.2.8",
    "cross-env": "^7.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  }
}
